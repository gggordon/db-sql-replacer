{
    "name": "gggordon/php-sql-replacer",
    "description": "Replace values in `SQL`  files especially with serialized php values. Useful for database migrations/clones.",
    "license": "MIT",
    "authors": [
        {
            "name": "ggordon",
            "email": "ggordon@igonics.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "PhpSqlReplacer\\": "src/"
        },
        "psr-4":{
            "PhpSqlReplacer\\External\\":"lib/"
        }
    },
    "bin": [
        "bin/php-sql-replacer.php",
        "bin/php-sql-replacer"
    ],
    "scripts":{
        "test": "./vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSqlReplacer\\Tests\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit":"5.7.*"
    }
}